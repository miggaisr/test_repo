{
  "type": "object",
  "properties": {
    "is_main": {
      "type": "identifier",
      "selector": "#main_layer",
      "control": "exists"
    },
    "goToGeneral": {
      "type": "action",
      "selector": ".main_menu_header",
      "control": "action.click",
      "selectContains": "General"
    }
  },
  "connect": [
    {
      "match": [
        "is_main"
      ],
      "action": "goToGeneral"
    }
  ]
}
