{
  "type": "object",
  "properties": {
    "logged_details": {
      "type": "string",
      "selector": "#logged",
      "control": "property.innerText"
    },
    "is_main": {
      "type": "identifier",
      "selector": "#logged",
      "control": "exists"
    }
  },
  "connect": [
    {
      "match": [
        "is_main"
      ],
      "screens": [
        "home"
      ],
      "model": "main_page"
    }
  ]
}
