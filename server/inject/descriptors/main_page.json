{
  "type": "object",
  "properties": {
    "is_main": {
      "type": "identifier",
      "selector": "#main_layer",
      "control": "exists"
    },
    "goToGeneral": {
      "type": "action",
      "selector": ".main_menu_header",
      "control": "action.click",
      "selectContains": "General"
    },
    "goToMs3Support": {
      "type": "action",
      "selector": ".menu_buttons",
      "control": "action.link",
      "selectContains": "MS3"
    },
    "is_general": {
      "type": "identifier",
      "selector": ".main_menu_header",
      "control": "exists",
      "selectContains": "General"
    },
    "logged_details": {
      "type": "string",
      "selector": "#logged",
      "control": "property.innerText"
    }
  },
  "connect": [
    {
      "match": [
        "is_main"
      ],
      "screens": [
        "home"
      ]
    },
    {
      "match": [
        "OBJECT.is_monitor"
      ],
      "screens": [
        "monitor"
      ]
    }
  ]
}
