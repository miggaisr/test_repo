{
  "type": "object",
  "properties": {
    "is_main": {
      "type": "string",
      "selector": "#main_layer",
      "control": "exists"
    },
    "logged_details": {
      "type": "string",
      "selector": "#logged",
      "control": "property.innerText"
    },
    "goToGeneral": {
      "type": "action",
      "selector": ".main_menu_header",
      "control": "action.click",
      "selectContains": "General"
    },
    "goToMs3Support": {
      "type": "action",
      "selector": ".menu_buttons",
      "control": "action.click",
      "selectContains": "MS3"
    }
  },
  "connect": [
    {
      "match": [],
      "action": "goToGeneral",
      "model": "main_page"
    },
    {
      "match": [],
      "action": "goToMs3Support",
      "model": "main_page"
    }
  ]
}
