{
  "type": "object",
  "properties": {
    "is_support": {
      "type": "identifier",
      "selector": ".menu_buttons",
      "control": "exists",
      "selectContains": "MS3"
    },
    "support_details": {
      "type": "string",
      "selector": ".work.frame_border",
      "control": "property.innerText",
      "selectContains": "Yaron"
    }
  },
  "connect": [
    {
      "match": [
        "is_support"
      ],
      "screens": [
        "home"
      ]
    }
  ]
}
