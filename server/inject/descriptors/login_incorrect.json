{
  "type": "object",
  "properties": {
    "is_login_incorrect": {
      "type": "identifier",
      "selector": ".error_m",
      "control": "exists",
      "selectContains": "Password is incorrect"
    },
    "login_again": {
      "type": "action",
      "control": "action.button",
      "selector": ".orange_button"
    }
  },
  "connect": [
    {
      "match": [
        "is_login_incorrect"
      ],
      "action": "login_again"
    }
  ]
}
